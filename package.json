{
  "name": "prosemaker",
  "version": "0.0.2",
  "description": "Document assembly for scientific reporting",
  "author": "Daniel Baird <daniel@danielbaird.com> (http://danielbaird.com)",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist/*",
    "lint": "standard",
    "build": "pegjs src/parser/pm.pegjs dist/pm.js && pegjs --trace src/parser/pm.pegjs dist/pm-trace.js",
    "dev": "npm run lint && npm run build && npm run test",
    "test": "npm build && node tests/test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DanielBaird/prosemaker.git"
  },
  "engines": {
    "node": "5.3.0"
  },
  "keywords": [
    "prosemaker",
    "prose",
    "maker",
    "document",
    "assembly"
  ],
  "author": "Daniel Baird <daniel@danielbaird.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DanielBaird/prosemaker/issues"
  },
  "homepage": "https://github.com/DanielBaird/prosemaker",
  "dependencies": {
    "pegjs": "^0.9.0"
  },
  "devDependencies": {
    "standard": "^6.0.5"
  },
  "standard": {
    "ignore": ["dist/"]
  }

}
